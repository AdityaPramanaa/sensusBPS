{
  "version": 2,
  "builds": [
    {
      "src": "app-vercel.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app-vercel.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "app-vercel.py": {
      "maxDuration": 30
    }
  }
} 